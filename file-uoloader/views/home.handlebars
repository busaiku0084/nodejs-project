{{!-- <h1>Hello template Engine!</h1> --}}

<form action="/" method="post" enctype="multipart/form-data">
  <h3>画像をアップロード</h3>
  <input type="file" name="imageFile" accept="image/*">
  <input type="submit">
</form>
<div>
  <p>↓保存された画像↓</p>
  {{#each rows}}
  <img src="{{this.image_name}}" alt="">
  {{/each}}
</div>
